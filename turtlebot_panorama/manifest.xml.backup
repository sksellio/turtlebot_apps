<package>
  <description brief="Use your TurtleBot to take panorama pictures">

     This app utilises pano_ros for taking snapshots and stitching them together to create panorama pictures.

  </description>
  <author>Younhoon Ju, Jihoon Lee and Marcus Liebhardt</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/turtlebot_panorama</url>
  <depend package="roscpp"/>
  <depend package="actionlib"/>
  <depend package="pano_ros"/>
  <depend package="image_transport"/>
  <depend package="std_msgs"/>
  <depend package="sensor_msgs"/>
  <depend package="geometry_msgs"/>
  <depend package="turtlebot_bringup"/>
  <depend package="yocs_velocity_smoother"/>
  <rosdep name="eigen"/>
  
  <depend package="std_srvs"/>
</package>


