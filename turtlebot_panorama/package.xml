<package>
  <name>turtlebot_panorama</name>
  <description>

     This app utilises pano_ros for taking snapshots and stitching them together to create panorama pictures.

  </description>
  <version>2.1.0</version>
  <maintainer email="jihoonlee.in@gmail.com">Jihoon Lee</maintainer>
  <license>BSD</license>
  <author>Younhoon Ju</author>
  <author>Jihoon Lee</author>
  <author>Marcus Liebhardt</author>

  <url>http://ros.org/wiki/turtlebot_panorama</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>pano_ros</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>turtlebot_bringup</build_depend>
  <build_depend>yocs_velocity_smoother</build_depend>
  <build_depend>std_srvs</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>pano_ros</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>turtlebot_bringup</run_depend>
  <run_depend>yocs_velocity_smoother</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>message_runtime</run_depend>

</package>


